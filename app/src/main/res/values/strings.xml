    <string name="instructionsHeaderString">Instructions</string>
    <string name="block_instructions"><li>Place your hand flat on the table.</li>
        \n\n<li>When prompted, grasp the block and place it on the top of the shelf.</li>
        \n\n<li>Return your hand to the table.</li>
    </string>
    <string name="water_instructions"><li>Place your hand flat on the table.</li>
        \n\n<li>When prompted, grasp the can and bring it up to your lips.</li>
        \n\n<li>Place the can back on the start position.</li>
        \n\n<li>Return your hand to the table.</li>
    </string>
    <string name="cup_instructions"><li>Place your hand flat on the table.</li>
        \n\n<li>On the green \'go\' signal, grasp the block and place it on the top of the shelf.</li>
        \n\n<li>Return your hand to the table.</li>
    </string>
    <string name="rod_instructions"><li>Place your hand flat on the table.</li>
        \n\n<li>When prompted, grasp the pencil and place it in the holder.</li>
        \n\n<li>Return your hand to the table.</li>
    </string>
